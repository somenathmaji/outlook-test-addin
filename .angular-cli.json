{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
      "name": "read"
    },
    "apps": [
      {
        "root": "src",
        "outDir": "dist",
        "assets": [
          "assets",
          "favicon.ico"
        ],
        "index": "index.html",
        "main": "taskpane.ts",
        "tsconfig": "tsconfig.app.json",
        "prefix": "app",
        "styles": [
          "taskpane.css"
        ]
      }
    ],
    "e2e": {
      "protractor": {
        "config": "./protractor.conf.js"
      }
    },
    "lint": [
      {
        "project": "src/tsconfig.app.json"
      }
    ],
    "test": {
      "karma": {
        "config": "./karma.conf.js"
      }
    },
    "defaults": {
      "styleExt": "css",
      "component": {}
    }
  }
  